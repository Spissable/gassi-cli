{
  "name": "gassi-cli",
  "description": "Run SYNC, QUERY, EXECUTE and DISCONNECT requests on Google SmartHome easily",
  "version": "0.1.4",
  "author": "Spiss, Lukas @Spissable",
  "bin": {
    "gassi": "./bin/run"
  },
  "bugs": "https://github.com/Spissable/gassi-cli/issues",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.2",
    "axios": "^0.21.1",
    "tslib": "^2.1.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@types/axios": "^0.14.0",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.25",
    "@types/uuid": "^8.3.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "globby": "^11.0.2",
    "jest": "^26.6.3",
    "nock": "^13.0.7",
    "prettier": "^2.2.1",
    "stdout-stderr": "^0.1.13",
    "ts-jest": "^26.5.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/Spissable/gassi-cli",
  "keywords": [
    "oclif",
    "google",
    "smarthome",
    "assistant",
    "cli"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "gassi",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "Spissable/gassi-cli",
  "scripts": {
    "format": "prettier --write ./src/",
    "lint": "eslint 'src/**/*.ts' --ignore-pattern node_modules/",
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "jest --coverage --detectOpenHandles ",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts",
  "volta": {
    "node": "12.10.0"
  }
}
