{
  "name": "gassi-cli",
  "description": "Run SYNC, QUERY, EXECUTE and DISCONNECT requests on Google SmartHome easily",
  "version": "0.1.1",
  "author": "Spiss, Lukas @Spissable",
  "bin": {
    "gassi": "./bin/run"
  },
  "bugs": "https://github.com/Spissable/gassi-cli/issues",
  "dependencies": {
    "@oclif/command": "^1.5.18",
    "@oclif/config": "^1.13.3",
    "@oclif/plugin-help": "^2.2.1",
    "axios": "^0.19.0",
    "tslib": "^1.10.0",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@types/axios": "^0.14.0",
    "@types/node": "^10.14.16",
    "@types/uuid": "^3.4.5",
    "babel-eslint": "^10.0.2",
    "eslint": "^6.2.2",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "globby": "^10.0.1",
    "prettier": "1.18.2",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/Spissable/gassi-cli",
  "keywords": [
    "oclif",
    "google",
    "smarthome",
    "assistant",
    "cli"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "gassi",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "Spissable/gassi-cli",
  "scripts": {
    "lint": "eslint 'src/**/*.ts' --ignore-pattern node_modules/",
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
